{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": false,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": ".",
    "declaration": true,
    "sourceMap": true,
    "pretty": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*.ts"],
  "exclude": []
}
